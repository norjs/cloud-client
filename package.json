{
  "name": "@norjs/cloud-client",
  "version": "1.3.2",
  "description": "Client for NorJS's cloud framework",
  "main": "dist/index.js",
  "scripts": {
    "compile-watch": "babel --watch --presets es2015 -d dist/ src/",
    "compile": "babel --presets es2015 -d dist/ src/",
    "prepare": "npm run -s compile",
    "pretest": "npm run -s compile",
    "test": "mocha --require babel-register test/unit/**/*.spec.js",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norjs/cloud-client.git"
  },
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "mocha": "^5.2.0",
    "sinon": "^7.2.2"
  },
  "dependencies": {
    "@norjs/debug": "^1.3.2",
    "lodash": "^4.17.11"
  }
}
